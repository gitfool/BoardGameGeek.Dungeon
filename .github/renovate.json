{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "local>gitfool/.github:renovate-default"
  ],
  "packageRules": [
    {
      "description": "Group reactive dependencies.",
      "matchSourceUrlPrefixes": [ "https://github.com/dotnet/reactive" ],
      "groupName": "reactive dependencies",
      "groupSlug": "reactive"
    },
    {
      "description": "Group serilog dependencies.",
      "matchSourceUrlPrefixes": [ "https://github.com/serilog/" ],
      "groupName": "serilog dependencies",
      "groupSlug": "serilog"
    }
  ]
}
